  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "author": "sebtoombs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebtoombs/winston-log-rotate-transport.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "lint": "eslint .",
